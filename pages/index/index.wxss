/* m-wrapper */
.m-wrapper {width:100vw; padding:1em 0; color:#757575;}
.m-wrapper>r {margin-right:5vw; font-size:1.4em; float:right;}
.m-wrapper>l {margin-left:5vw; font-size:0.8em;  }
.m-wrapper>l>span { margin-right:0.2em; color:#00bcd4;}

/* mass_pack */
.mass_pack {width:100vw; position:fixed; bottom:0; z-index:990;}
.mass_pack>.mass {width:100vw; height:100%; background:rgba(0,0,0,0.3); position:fixed; bottom:0; z-index:991;}
.mass_pack>.show_pack {width:100vw; height:30vh; background:#fff; position:fixed; bottom:0; z-index:992;}


